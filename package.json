{
  "name": "rx-redux",
  "version": "0.5.3",
  "description": "Reimplement redux using rxjs",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib dist",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/rx-redux.js --display-modules --progress && NODE_ENV=production webpack src/index.js dist/rx-redux.min.js --display-modules --progress",
    "build": "npm run build:lib && npm run build:umd",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jas-chen/rx-redux.git"
  },
  "keywords": [
    "rx-redux",
    "redux",
    "reactive",
    "rx",
    "rxjs",
    "flux"
  ],
  "author": "Jas Chen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jas-chen/rx-redux/issues"
  },
  "homepage": "https://github.com/jas-chen/rx-redux#readme",
  "devDependencies": {
    "babel-core": "^5.8.3",
    "babel-eslint": "^4.0.5",
    "babel-loader": "^5.3.2",
    "eslint": "^0.24.1",
    "eslint-config-airbnb": "0.0.6",
    "rimraf": "^2.4.2",
    "rx": "^2.5.3",
    "webpack": "^1.10.3"
  }
}
